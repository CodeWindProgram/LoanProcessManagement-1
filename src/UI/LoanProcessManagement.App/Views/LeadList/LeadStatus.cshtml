                    <input type="hidden" name="ctl00$ContentPlaceHolder1$hdnEmpId" id="hdnEmpId" value="HO">
                    <div class="filters">
                        <table style="width: 100%">
                            <tbody>
                                <tr style="border: none;">
                                    <td style="width: 25%;">
                                        Zone
                                        <div id="divZone">
                                            <select id="DDLZone" class="form-control">
                                                <option value="-1">Select</option>
                                            </select>
                                            <input type="hidden" name="ctl00$ContentPlaceHolder1$HDNddlZone" id="ContentPlaceHolder1_HDNddlZone">
                                        </div>
                                    </td>
                                    <td style="width: 25%; display: none;">
