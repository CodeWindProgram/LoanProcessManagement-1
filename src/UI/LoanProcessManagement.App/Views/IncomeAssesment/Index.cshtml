                                    <h4><u><b>Customer Details</b></u></h4>
                                </div>
                                <div class="form-group p-3">
                                    <div class="row">
                                        <div class="col-lg-4 col-sm-12">
                                            <label class="">Name</label>
                                            <p>@Model.CustomerName</p>
                                        </div>

                                        <div class="col-lg-4 col-sm-12">
                                            <label class="">Customer Email</label>
                                            <p>@Model.CustomerEmail</p>
                                        </div>

                                        <div class="col-lg-4 col-sm-12">
                                            <label class="">Customer Mobile</label>
                                            <p>@Model.CustomerPhone</p>
                                        </div>


                                    </div>
                                </div>

                                <div class="form-group p-3">
                                    <div class="row">
                                        <div class="col-lg-4 col-sm-12">
                                            <label class="">Employee Type</label>
                                            <p>@Model.EmploymentType</p>
                                        </div>

                                        <div class="col-lg-4 col-sm-12">
                                            <label class="">No. of Bank Accounts</label>
                                            <p id="idNoOfBankAccounts">@Model.NoOfBankAccounts</p>
                                        </div>

                                        <div class="col-lg-4 col-sm-12">
                                            <label class="">No. of Bank Details Uploaded</label>
                                            <p id="idNoOfBankAccUpld">@Model.IncomeRecords</p>
                                        </div>
                                    </div>
                                </div>


                                <div id="IdentificationDv">
