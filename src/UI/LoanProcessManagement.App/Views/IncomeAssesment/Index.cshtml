                                    <div class="row tab">
                                        <div class="col-md-5">
                                            <div class="input-control text">
                                                <input id="radGenerateLink" asp-for="FileType" type="radio" name="FileType" value="GenerateLink" checked="checked" />
                                                Generate Link
                                                &nbsp;&nbsp;&nbsp;
                                                <input id="radFileUpload" asp-for="FileType" type="radio" name="FileType" value="FileUpload" />
                                                Upload File
                                                <span asp-validation-for="FileType" class="text-danger"></span>
                                            </div>

                                        </div>
                                    </div>
                                </div>
                                <div id="divFileUpload" style="display : none">
                                    <div id="divFillDetails">
                                        <hr />
                                        <h4><u><b>Fill Details</b></u></h4>
                                    </div>
                                    <div class="pt-2" style="height: 5px;"></div>
                                    <div class="form-group">
                                        <div class="row">

                                            <div class="col-lg-4  col-sm-12">
                                                <label asp-for="Institution_Id" class="control-label">Institution Name<span style="color: Red">&nbsp;*</span></label>

                                                <div class="input-control text">
                                                    <select asp-for="Institution_Id" class="form-control" asp-items="ViewBag.institutions">
                                                        <option value="">Select Institution</option>
                                                        @*<option value="1">AU Small Finance Bank</option>
                                                        <option value="2">Acme Bank Ltd</option>
                                                        <option value="3">Adarsh Bank</option>
                                                        <option value="4">Ahmedabad District Co-Op</option>
                                                        <option value="5">Airtel Payments Bank</option>*@
                                                    </select>
                                                    <span asp-validation-for="Institution_Id" class="text-danger"></span>
                                                </div>
                                            </div>

                                            <div class="col-lg-4 col-sm-12">
                                                <label asp-for="DocumentType" class="control-label">Document Type<span style="color: Red">&nbsp;*</span></label><br />

                                                <input id="radScannedDocument" asp-for="DocumentType" type="radio" name="DocumentType" value="ScannedDocument" />
                                                Scanned Document
                                                &nbsp;&nbsp;&nbsp;
                                                <input id="radStatement" asp-for="DocumentType" type="radio" name="DocumentType" value="e-Statement" />
                                                e-Statement<br />
                                                <span asp-validation-for="DocumentType" class="text-danger"></span>
                                            </div>

                                        </div>
                                    </div>
                                    <div class="form-group">
                                        <div class="row">

                                            <div class="col-lg-4 col-sm-12">
                                                <label asp-for="IPdf" class="control-label">Upload File (PDF Only)<span style="color: Red">&nbsp;*</span></label>

                                                <input type="file" asp-for="IPdf" id="fileUploadPdf" class="form-control" accept=".pdf" onchange="validate_fileupload(this,&#39;pdf&#39;);">
                                                <span asp-validation-for="IPdf" class="text-danger"></span>
                                            </div>

                                            <div class="col-lg-4 col-sm-12">
                                                <label asp-for="FilePassword" class="control-label">PDF File Passwords (if any) @*<span style="color: Red">&nbsp;*</span>*@</label>

                                                <input type="password" asp-for="FilePassword" class="form-control" placeholder="Pdf Password" />
                                                <span asp-validation-for="FilePassword" class="text-danger"></span>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <br />
                                <div class="row tab pb-3">
                                    <div class="col-md-12">
                                        <center>
                                            <input type="submit" name="btnAddIncomeDetails" value="Submit" id="btnSubmitIncomeDetails" class="btn" style="background-color: #343a40; color:white;">&nbsp
                                            <a id="cancelbutton" class="btn btn-danger" href="/LeadList/LeadSummary/@Model.LeadID">Cancel</a>
                                        </center>
                                    </div>
                                </div>

                                <div id="divIncomeRecords">
                                    <div>
                                        <hr />
                                        <h4><u><b>Income Assessment Records</b></u></h4>
                                    </div>
                                    <div>
                                        @await Component.InvokeAsync("IncomeAssessmentRecordsList", new { applicantType = Model.ApplicantType, lead_Id = Model.lead_Id })
                                    </div>
                                </div>
                            </div>
                        </div>
                    </form>
                </div>
            </div>
        </div>
    </div>
</div>

