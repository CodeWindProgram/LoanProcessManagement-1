<script>

    $(document).ready(function () {
        //debugger
        if ('@isSuccess'.toLowerCase() == 'True'.toLowerCase()) {
            toastr.success('@Message', "Success");
        }
    });



    $(document).ready(function () {

        $("#startDate").datepicker({
            showOn: "button",
            changeMonth: true,
            changeYear: true,
            format: "yyyy-mm",
            endDate: '-12m',
            startDate: '-15m',
            viewMode: "months",
            minViewMode: "months",
            //format: "dd-mm-yyyy",
            //dateFormat: 'dd-mm-yy'
            yearRange: "-100:+0"
        }).on('changeDate',function (ev) {

            var month = ev.date.getMonth();
            var year = ev.date.getFullYear();

            var selectedDate = new Date(year, month, 1);
            selectedDate.setMonth(selectedDate.getMonth() + 11);

            let monthTemp = selectedDate.getMonth() + 1;

            if (monthTemp < 10) {
                monthTemp = "0" + monthTemp;
            }

            $("#endDate").val(selectedDate.getFullYear() + "-" + monthTemp).datepicker({
                changeMonth: true,
                changeYear: true,
                format: "yyyy-mm",
                viewMode: "months",
                minViewMode: "months",
                yearRange: "-100:+0"
            });
        });

        $('#btnClear').click(function () {

                $('#startDate').val('');
                $('#endDate').val('');

        });

        $(function () {
            //debugger
            var jsArray = @Html.Raw(Json.Serialize(Model.AppTypeList));
            for (var i = 0; i < jsArray.length; i++) {
                var id = "#lnkCoApp_" + jsArray[i];
                $(id).css('pointer-events', 'auto');
                //$(id).css("display", "inline-block");
                //$(id).show();
            }
        });
    });

    $(function () {
        $(".noTxtInput").keypress(function (e) {
            var keyCode = e.which;
            /* 8 - (backspace), 32 - (space), 48-57 - (0-9) - Numbers */
            if ((keyCode != 8 || keyCode == 32) && (keyCode < 48 || keyCode > 57)) {
                return false;
            }
        });
    });

    $(document).ready(function () {

        if ('@ViewBag.applicantTypeNo' == 0) {
            $("#l0").removeClass("active");
            $("#l0").addClass("active");


        }

        if ('@ViewBag.applicantTypeNo' == 1) {
            $("#l1").removeClass("active");
            $("#l1").addClass("active");


        }

        else if ('@ViewBag.applicantTypeNo' == 2) {
            $("#l2").removeClass("active");
            $("#l2").addClass("active");

        }

        else if ('@ViewBag.applicantTypeNo' == 3) {
            $("#l3").removeClass("active");
            $("#l3").addClass("active");


        }

        else if ('@ViewBag.applicantTypeNo' == 4) {
            $("#l4").removeClass("active");
            $("#l4").addClass("active");


        }
        else if ('@ViewBag.applicantTypeNo' == 5) {
            $("#l5").removeClass("active");
            $("#l5").addClass("active");


        }
    });

    $('.AppTabs').click(function (e) {
        $(".AppTabs").removeClass("selected");
        $(e.target).addClass("selected");
    });

    $(function () {
        $("input[name='FileType']").change(function () {
            if ($("#radFileUpload").is(":checked")) {
                $("#divFileUpload").show();
            } else {
                $("#divFileUpload").hide();
            }
        });
    });

    //debugger
    var labelVal = $("#idNoOfBankAccounts").text();
    if (@Model.NoOfBankAccounts <= @Model.IsSubmitCount) {
        $("#btnSubmitIncomeDetails").attr('disabled', 'disabled');
    }

    function validate_fileupload(filename, extvalues) {
        //debugger;
        var allowedExt = extvalues.split(',');
        var filearr = filename.value.toLowerCase().split('.');
        var fileext = filearr[filearr.length - 1];
        if (allowedExt.includes(fileext)) {
            return true;
        }
        else {
            alert("Please Select File of " + extvalues + " Extension Format Only");
            filename.value = "";
            return false;
        }
    }

    if (@Model.IncomeRecords == 0) {
        $("#divIncomeRecords").hide()
    }
    else {
        $("#divIncomeRecords").show()
    }
</script>





