                                                <input asp-for="StartDate" class="form-control noTxtInput" id="startDate" placeholder="mm-yyyy" onkeydown="return /[a-z]/i.test(event.key)" autocomplete="off" />

                                            </div>

                                            <div class="col-lg-4 col-sm-12">
                                                <label asp-for="EndDate" class="control-label">Statement End Month</label>

                                                <input asp-for="EndDate" class="form-control noTxtInput" id="endDate" placeholder="mm-yyyy" onkeydown="return /[a-z]/i.test(event.key)" readonly />

                                            </div>

                                            <div class="col-lg-2 col-sm-12">
                                                <label>&nbsp;&nbsp;</label>
                                                <input type="reset" class="btn btn-primary" value="Clear Date Filter" id="btnClear" />
                                            </div>
                                        </div>
                                    </div>
                                </div>

