<div class="container pb-5">
    <div class="container app">
        <div class="col-sm-12 col-lg-12">
            <div data-effect="helix" id="panels" class="panel panel-info effect-helix in">
                <div class="panel-heading">
                    <h5>Add Customer Enquiry</h5>
                </div>
                <div class="formDiv">
                    <form method="post" action="/CibilCheckDetails/index">
                        <input type="hidden" asp-for="lead_Id" id="hdnLeadId">
                        <input type="hidden" asp-for="ApplicantType" id="hdnApplicantType">
                        <input type="hidden" asp-for="CreatedBy" value=@User.FindFirst("lg_id").Value />
                        <input type="hidden" asp-for="LastModifiedBy" value=@User.FindFirst("lg_id").Value />
                        <div style="height: 15px;"></div>
                        <div class="frr pt-4 pl-4 pr-4">
                            <a href="/CibilCheckDetails/Index?lead_Id=@Model.lead_Id&applicantType=0" class="AppTabs btn-sm selected" id="tabProcess0" style="background: #adb5bd;">Applicant</a>
                            <a id="ContentPlaceHolder1_lnkCoApp1" class="AppTabs btn-sm" href="/CibilCheckDetails/Index?lead_Id=@Model.lead_Id&applicantType=1" style="background: #343a40;">Co-Applicant-1</a>
                            <a id="ContentPlaceHolder1_lnkCoApp2" class="AppTabs btn-sm" href="/CibilCheckDetails/Index?lead_Id=@Model.lead_Id&applicantType=2" style="background: #343a40;">Co-Applicant-2</a>
                            <a id="ContentPlaceHolder1_lnkCoApp3" class="AppTabs btn-sm" href="/CibilCheckDetails/Index?lead_Id=@Model.lead_Id&applicantType=3" style="background: #343a40;">Co-Applicant-3</a>
                            <a id="ContentPlaceHolder1_lnkCoApp4" class="AppTabs btn-sm" href="/CibilCheckDetails/Index?lead_Id=@Model.lead_Id&applicantType=4" style="background: #343a40;">Co-Applicant-4</a>
                            <a id="ContentPlaceHolder1_lnkCoApp5" class="AppTabs btn-sm" href="/CibilCheckDetails/Index?lead_Id=@Model.lead_Id&applicantType=5" style="background: #343a40;">Co-Applicant-5</a>
                        </div>

                        <span id="ContentPlaceHolder1_spanMessage" style="color: red;"></span>

                        <div class="frames">
                            <div class="panel-body">

                                <div id="ContentPlaceHolder1_alertMsg">
                                    <div class="row tab pt-3 pr-1">
                                        <div class="col-md-12 text-center">
                                            <label class="h4">Form No.</label>



                                            <span id="ContentPlaceHolder1_lblFormNo" class="h4" style="color: black; border:none">
                                                <label>@Model.FormNo</label>
                                                <input type="hidden" asp-for="FormNo" />

                                            </span>

                                        </div>
                                    </div>
                                </div>
                                <hr />
                                <div style="display:flex;">
                                    <div class="col-md-4">

                                    </div>

                                </div>
                                <div id="">
                                    <h4><u><b>Customer Details</b></u></h4>
                                </div>

                                <div class="form-group pl-2">
                                    <div class="row">
                                        <div class="col-lg-4 col-sm-12">
                                            <label class="">Customer Name</label>
                                            <p>@Model.CustomerName</p>
                                        </div>



                                        <div class="col-lg-4 col-sm-12">
                                            <label class="">Customer Email</label>
                                            <p>@Model.CustomerEmail</p>
                                        </div>




                                        <div class="col-lg-4 col-sm-12">
                                            <label class="">Customer Mobile</label>
                                            <p>@Model.CustomerPhone</p>
                                        </div>


                                    </div>
                                </div>

                                <div class="form-group pl-2">
                                    <div class="row">
                                        <div class="col-lg-4 col-sm-12">
                                            <label class="">Date of Birth</label>
                                            <p>@Model.DateOfBirth.ToShortDateString()</p>
                                        </div>




                                        <div class="col-lg-4 col-sm-12">
                                            <label class="">Gender</label>
                                            <p>@Model.Gender</p>
                                        </div>

                                    </div>
                                </div>





                                <hr />
                                <div id="IdentificationDv">
                                    <div style="height: 5px;"></div>
                                    <div id="divIdentification">
                                        <h4><u><b>Identification</b></u></h4>

                                    </div>

                                    <div class="form-group pl-2">
                                        <div class="row">
                                            <div class="col-lg-4 col-sm-12">
                                                <label class="">PAN</label>
                                                <p>@Model.PanCardNo</p>
                                            </div>

                                            <div class="col-lg-4 col-sm-12">
                                                <label class="">Passport</label>
                                                <p>@Model.PassportNo</p>
                                            </div>



                                            <div class="col-lg-4 col-sm-12">
                                                <label class="">Voter ID</label>
                                                <p>@Model.VoterId</p>
                                            </div>
                                        </div>
                                    </div>

                                    <div class="form-group pl-2">
                                        <div class="row">
                                            <div class="col-lg-4 col-sm-12">
                                                <label class="">Driver's Licence</label>
                                                <p>@Model.DrivingLiscenceNo</p>
                                            </div>



                                            <div class="col-lg-4 col-sm-12">
                                                <label class="">Ration Card</label>
                                                <p>@Model.RationCardNo</p>
                                            </div>

                                            <div class="col-lg-4 col-sm-12">
                                                <label class="">Universal ID</label>
                                                <p>@Model.AadharId</p>
                                            </div>
                                        </div>
                                    </div>




                                </div>
                                <hr />
                                <div id="PhoneDV">
                                    <div style="height: 5px;"></div>
                                    <div>
                                        <h4><u><b>Phone</b></u></h4>
                                    </div>

                                    <div class="form-group pl-2">
                                        <div class="row">
                                            <div class="col-lg-4 col-sm-12">
                                                <label class="">Phone 1</label>
                                                <p>@Model.PhoneNumber1</p>
                                            </div>



                                            <div class="col-lg-4 col-sm-12">
                                                <label class="">Phone 2</label>
                                                <p>@Model.PhoneNumber2</p>
                                            </div>
                                        </div>
                                    </div>


                                </div>

                                <hr />
                                <div id="currentAddr">
                                    <div id="divConverted4">
                                        <h4><u><b>Address</b></u></h4>
                                    </div>

                                    <div class="form-group pl-2">
                                        <div class="row">
                                            <div class="col-lg-4 col-sm-12">
                                                <label class="">Address Line 1</label>
                                                <p>@Model.AddressLine1</p>
                                            </div>



                                            <div class="col-lg-4 col-sm-12">
                                                <label class="">Address Line 2</label>
                                                <p>@Model.AddressLine2</p>
                                            </div>



                                            <div class="col-lg-4 col-sm-12">
                                                <label class="">Address Line 3</label>
                                                <p>@Model.AddressLine3</p>
                                            </div>

                                        </div>
                                    </div>

                                    <div class="form-group pl-2">
                                        <div class="row">
                                            <div class="col-lg-4 col-sm-12" id="dvStateCurrent">
                                                <label class="">State</label>
                                                <p id="StateNode"></p>

                                            </div>



                                            <div class="col-lg-4 col-sm-12">
                                                <label class="">City</label>
                                                <p>@Model.City</p>
                                            </div>



                                            <div class="col-lg-4 col-sm-12">
                                                <label class="">Pin Code</label>
                                                <p>@Model.Pincode</p>
                                            </div>
                                        </div>
                                    </div>

                                    <div class="form-group pl-2">
                                        <div class="row">
                                            <div class="col-lg-4 col-sm-12">
                                                <label class="">Category</label>
                                                <p id="CategoryNode"></p>
                                            </div>





                                            <div class="col-lg-4 col-sm-12">
                                                <label class="">Residence code</label>
                                                <p id="ResidenceNode"></p>
                                            </div>


                                            <div class="col-lg-4 col-sm-12">
                                                <label class="">Qualification</label>
                                                <p id="QualificationNode"></p>
                                            </div>
                                        </div>
                                    </div>
                                </div>




                            </div>
                        </div>

                    </form>
                </div>

            </div>
        </div>
    </div>
</div>

