                                                <p>@Model.DrivingLiscenceNo</p>
                                            </div>



                                            <div class="col-lg-4 col-sm-12">
                                                <label class="">Ration Card</label>
                                                <p>@Model.RationCardNo</p>
                                            </div>

                                            <div class="col-lg-4 col-sm-12">
                                                <label class="">Universal ID</label>
                                                <p>@Model.AadharId</p>
                                            </div>
                                        </div>
